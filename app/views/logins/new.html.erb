
<div class="continer">
  <div class="row center sign-up-form">
    <div class="col s12">
      <div class="card-panel">
           <h3 class="header">Log In</h3>
            <div class="row">
                <%= form_for(:logins,html: {class: "col s12"})  do |f| %>
                  
                   </div>
                   <div class="row">
                      <div class="input-field col s12">
                            <%=f.text_field:email,placeholder:"enter your eamil",autofocus:true%>
                            <%= f.label:email%>
                      </div>
                    </div>
                     <div class="row">
                      <div class="input-field col s12">
                            <%=f.password_field:password,placeholder:"enter your password",autofocus:true%>
                            <%= f.label:password  %>
                      </div>
                    </div>
                 
                    <div class="row">
                      <div class="input-field col s12">
                             
                             <%=f.button( '<i class="material-icons right">arrow_forward</i> Login'.html_safe  , class:"btn orange waves-effect waves-light right")%>
                        </div>
                    </div>
                <%end%>
            </div>
       </div>
    </div>
  </div>
 </div>