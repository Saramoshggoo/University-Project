<div class='row'>
    <div class="col s12 .center-align">
    <h3 class="center-align grey-text text-darken-2"> Student Directory</h3>

   <div class="row">
       <% @students.each do |student|%>
        <div class="col s6 m6">
        <div class="card grey darken-1">
            <div class="card-content white-text">
            <span class="card-title"> Name:<%=link_to student.name ,student_path(student)%> </span>
             <span > Email<%=student.email%></span>
            <p>Course Enrollments :none</p>
            </div>
            <div class="card-action">
            <%=link_to "see profile",student_path(student)%>
           
            </div>
        </div>
        </div>
        <%end%>
  </div>
            
    
    </div>
</div>